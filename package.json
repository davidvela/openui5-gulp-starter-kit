{
  "name": "openui5-gulp-starter-kit",
  "version": "0.0.2",
  "description": "OpenUI5 template app build with gulp.",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "repository": {
    "url": "git+https://github.com/pulseshift/openui5-gulp-starter-kit.git",
    "type": "git"
  },
  "authors": [
    "Jascha Quintern <jascha.quintern@pulseshift.com>",
    "Michael Dell <michael.dell@pulseshift.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pulseshift/openui5-gulp-starter-kit/issues"
  },
  "homepage": "https://github.com/pulseshift/openui5-gulp-starter-kit#readme",
  "main": "src/index.handlebars",
  "ui5": {
    "indexTitle": "UI5 Todo App",
    "src": "cdn",
    "srcLinks": {
      "github": {
        "url": "https://github.com/SAP/openui5/archive/1.48.0.zip",
        "version": "1.48.0",
        "isPrebuild": false,
        "isArchive": true
      },
      "cdnDownload": {
        "url": "https://openui5beta.hana.ondemand.com/downloads/openui5-sdk-1.48.2.zip",
        "version": "1.48.2",
        "isPrebuild": true,
        "isArchive": true
      },
      "cdn": {
        "url": "https://openui5.hana.ondemand.com/1.48.2/resources/sap-ui-core.js",
        "version": "1.48.2",
        "isPrebuild": true,
        "isArchive": false
      },
      "local": {
        "url": "ui5/your-local-build/sap-ui-core.js",
        "version": "1.48.2",
        "isPrebuild": true,
        "isArchive": false
      }
    },
    "apps": [
      {
        "name": "app.todo",
        "path": "src/openui5-todo-app"
      }
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "del": "^3.0.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^3.4.2",
    "gulp-cli": "^1.3.0",
    "gulp-download": "fuchsvomwalde/gulp-download",
    "gulp-handlebars-html": "^0.0.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-less": "^3.3.2",
    "gulp-plumber": "^1.1.0",
    "gulp-pretty-data": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tap": "^1.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-ui5-preload": "^1.3.1",
    "gulp-unzip": "^0.2.0",
    "gulp-util": "^3.0.8",
    "handlebars": "^4.0.10",
    "husky": "^0.14.1",
    "less": "^2.7.2",
    "less-openui5": "^0.2.0",
    "less-plugin-autoprefix": "^1.5.1",
    "lint-staged": "^4.0.0",
    "loader-utils": "^1.1.0",
    "prettier": "^1.5.1"
  }
}
